<!doctype html>
<html ng-app="loginModule">

<head>
    <meta charset="utf-8">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="lib/bootstrap/dist/css/bootstrap.min.css" />

    <link rel="stylesheet" href="/styles/main.css">
    <link rel="stylesheet" href="/styles/sb-admin-2.css">
    <link rel="stylesheet" href="/styles/timeline.css">
    <link rel="stylesheet" href="/styles/custom.css">
    <link rel="stylesheet" href="lib/metisMenu/dist/metisMenu.min.css">
    <link rel="stylesheet" href="lib/angular-loading-bar/build/loading-bar.min.css">
    <link rel="stylesheet" href="lib/font-awesome/css/font-awesome.min.css" type="text/css">
</head>

<body ng-controller="loginController" ng-init="initializeController()">

    <div class="container">
        <div id="wrapper" class="row">
            <div id="login" class="col-md-8 col-md-offset-2" class="animate form">
                <section class="login_content">
                    <div class="login-panel panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title">Please Sign In</h3>
                        </div>
                        <div class="panel-body">
                            <form role="form">
                                <fieldset>
                                    <div class="form-group">
                                        <input ng-model="UserName" name="userNameWatch" type="text" placeholder="UserName" class="form-control" validator="required,email" valid-method="blur" />
                                    </div>
                                    <div class="form-group">
                                        <input ng-model="Password" name="passwordWatch" type="password" placeholder="Password" class="form-control" validator="required" valid-method="blur" />
                                    </div>

                                    <!-- Change this to a button or input when using this as a form -->
                                    <a class="btn btn-lg btn-success btn-block" ng-click="login(loginForm)"> Log in</a>
                                    <br/>
                                    <div class="separator">
                                        <p class="change_link">
                                            New to site?
                                            <a href="/register" class="to_register"> Create Account </a>
                                        </p>
                                    </div>
                                </fieldset>
                            </form>
                        </div>
                    </div>
                </section>
                <div>
                    <p>Â©2015 All Rights Reserved. Fictitious Company. Privacy and Terms</p>
                </div>
            </div>
        </div>
    </div>
    <script src="lib/jquery/dist/jquery.min.js "></script>
    <script src="lib/angular/angular.min.js "></script>
    <script src="lib/bootstrap/dist/js/bootstrap.min.js "></script>
    <script src="lib/angular-ui-router/release/angular-ui-router.min.js "></script>
    <script src="lib/json3/lib/json3.min.js "></script>
    <script src="lib/oclazyload/dist/ocLazyLoad.min.js "></script>
    <script src="lib/angular-loading-bar/build/loading-bar.min.js "></script>
    <script src="lib/angular-bootstrap/ui-bootstrap-tpls.min.js "></script>
    <script src="lib/metisMenu/dist/metisMenu.min.js "></script>
    <script src="lib/Chart.js/Chart.min.js "></script>
    <script src="app/app.js "></script>
    <script src="js/sb-admin-2.js "></script>

    <script src="lib/angular-route/angular-route.js "></script>
    <!--   <script src="lib/angular-block-ui/dist/angular-block-ui.min.js "></script>-->
    <script src="lib/angular-resource/angular-resource.min.js "></script>
    <script src="lib/angular-cookies/angular-cookies.min.js "></script>

    <script src="lib/angular-sanitize/angular-sanitize.js "></script>
    <script src="lib/angular-loading-bar/build/loading-bar.js "></script>
    <script src="lib/angular-validation/dist/angular-validation.min.js "></script>
    <script src="lib/angular-validation/dist/angular-validation-rule.min.js "></script>
    <!--   <script src="lib/angular-ui/build/angular-ui.min.js "></script>-->
    <script src="lib/angular-confirm-modal/angular-confirm.js "></script>
    <script src="lib/angular-loading-bar/build/loading-bar.js "></script>
    <script src="app/Services/accountsService.js "></script>
    <script src="app/Services/applicationService.js "></script>
    <script src="app/Services/AlertsService.js "></script>
    <script src="app/Services/interceptorService.js "></script>
    <script src="app/Accounts/Controllers/loginController.js "></script>
</body>

</html>